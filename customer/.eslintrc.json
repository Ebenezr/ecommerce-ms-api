{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        // "airbnb-base", "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "class-methods-use-this": 0,
        "max-len": [1, 140, 2, {
            "ignoreComments": true}],
        "no-param-reassign": 0,
         "quotes": ["error", "double", {"allowTemplateLiterals": true}],
//          "@typescript-eslint/quotes": [
//   "error",
//   "single",
//   {
//     "allowTemplateLiterals": true
//   }
// ]

    }
}
